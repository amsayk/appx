  "scripts": {
    "server:dev": "nodemon --watch js/ --watch server/ --watch public/ --watch data/ --watch backend/ -r dotenv/config -r ./server/env --inspect ./server/main.js --exec node",
    "link": "pushd dist && for l in `ls ../public`; do ln -sf ../public/`basename $l` `basename $l`; done && popd",
    "client:dev": "webpack-dev-server",
    "build-joyride-sass": "sass --no-cache js/containers/RootContainer/react-joyride.scss js/containers/RootContainer/react-joyride-compiled.css",
    "start": "npm run clean && npm run build && npm run link && NODE_ENV=production node -r dotenv/config -r ./server/iso/env server/main",
    "build": "NODE_ENV=production webpack --progress",
    "clean": "pushd dist && rm -fr * && popd"
  },
